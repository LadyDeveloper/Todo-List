<%- include("header") -%>
    <div class="box" id="heading">
        <h1> <%= kindOfDay %> </h1>
    </div>
    <div class="box">
        <% for(let i = 0; i < itemsList.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p> <%= itemsList[i] %> </p>
            </div>
        <% } %>
  
        <form class="item" action="/" method="post">
            <input type="text" name="itemTodo" placeholder="New Task" autocomplete="off">
            <button type="submit" name="button">+</button>
        </form>
    </div>  
<%- include("footer") -%>
